generator client {
  provider = "prisma-client-js"
  url      = env("DATABASE_URL")
}


datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id       Int      @id @default(autoincrement())
  email    String   @unique
  password String
  role     Role     @default(USER)
}

enum Role {
  ADMIN
  USER
}
